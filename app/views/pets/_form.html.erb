<div class="container text-center petform">
  <div>
    <h1 class="branch">Giving Pet Form</h1>
  </div>
<br>
<%= form_for @pet do |f| %>

  <%= f.label :name%>
  <%= f.text_field :name %><br>
  <br>
  <%= f.label "Animal Type"%>
  <%= f.select("animal_type", ["Cat", "Dog", "Rabbit", "Hamster"]) %><br>
  <br>
  <%= f.label :breed%>
  <%= f.text_field :breed %><br>
  <br>
  <%= f.label :age%>
  <%= f.number_field :age %><br>
  <br>
  <%= f.label :gender%>
  <%= f.select("gender", ['Male', 'Female']) %><br>
  <br>
  <%= f.hidden_field :store_id %>
  <br>


  <%= f.submit "Give Your Pet",class: "btn btn-primary" %>
<% end %>
</div>
